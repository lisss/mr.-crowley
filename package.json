{
    "name": "crowley",
    "version": "1.0.0",
    "scripts": {
        "build": "npm run build:less && tsc && npm run bundle",
        "build:less": "lessc static/css/styles.less static/css/styles.css",
        "bundle": "node scripts/bundle.js",
        "watch": "tsc --watch",
        "watch:less": "lessc --watch static/css/styles.less static/css/styles.css"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "less": "^4.4.2",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
